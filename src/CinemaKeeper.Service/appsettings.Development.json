{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/service.log"
        }
      }
    ]
  },
  "Service": {
    "ModulesAssemblyNamePattern": "CinemaKeeper.Modules*.dll",
    "ModuleAssembliesDirectory": "Modules",
    "ModulesUpdateRate": "01:00:00"
  },
  "DiscordBot": {
    "Prefix": "!"
  }
}
